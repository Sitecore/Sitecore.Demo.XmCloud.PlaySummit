  "scripts": {
    "DEMO_TEAM_CUSTOMIZATION_EXTRA_SCRIPTS": "",
    "storybook": "npm-run-all --serial bootstrap storybook:start",
    "storybook:start": "storybook dev -p 6006",
    "build-storybook": "npm-run-all --serial bootstrap storybook:build",
    "storybook:build": "storybook build",
    "eslint": "eslint ./src/**/*.tsx ./src/**/*.ts ./scripts/**/*.ts",
    "stylelint": "stylelint ./src/**/*.css",
    "build:local": "cross-env NEXT_PUBLIC_DISCOVER_CUSTOMER_KEY=0-0 NEXT_PUBLIC_DISCOVER_API_KEY=0-0-0 NEXT_PUBLIC_ORDERCLOUD_BUYER_CLIENT_ID=00000000-0000-0000-0000-000000000000 NEXT_PUBLIC_ORDERCLOUD_BASE_API_URL=https://sandboxapi.ordercloud.io npm-run-all build",
    "analyze": "npm-run-all --serial bootstrap analyze:build",
    "analyze:build": "cross-env ANALYZE=true next build",
    "END_CUSTOMIZATION_EXTRA_SCRIPTS": "",
    "install-pre-push-hook": "ts-node --project tsconfig.scripts.json ./scripts/install-pre-push-hook.ts",
    "jss": "jss",
    "DEMO_TEAM_CUSTOMIZATION_LINT": "Run both eslint and stylelint",
    "lint": "npm-run-all --serial eslint stylelint",
    "END_CUSTOMIZATION_LINT": "",
    "bootstrap": "ts-node --project tsconfig.scripts.json scripts/bootstrap.ts",
    "build": "npm-run-all --serial bootstrap next:build",
    "DEMO_TEAM_CUSTOMIZATION_GRAPHQL_ENDPOINT": "Hardcode the GraphQL endpoint",
    "graphql:update": "cross-env GRAPH_QL_ENDPOINT=https://cm.xmcloudcm.localhost/sitecore/api/graph/edge ts-node --project tsconfig.scripts.json ./scripts/fetch-graphql-introspection-data.ts",
    "END_CUSTOMIZATION_GRAPHQL_ENDPOINT": "",
    "next:build": "next build",
    "next:dev": "cross-env NODE_OPTIONS='--inspect' next dev",
    "next:start": "next start",
    "scaffold": "ts-node --project tsconfig.scripts.json scripts/scaffold-component/index.ts",
    "start:connected": "npm-run-all --serial bootstrap --parallel next:dev start:watch-components",
    "start:production": "npm-run-all --serial bootstrap next:build next:start",
    "start:watch-components": "ts-node --project tsconfig.scripts.json scripts/generate-component-builder/index.ts --watch"
  }
}
